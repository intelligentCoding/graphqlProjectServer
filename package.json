{
  "name": "graph-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc && yarn replace:paths",
    "replace:paths": "tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon dist/index.js",
    "start": "tsc && node dist/index.js",
    "migration:generate": "yarn build && typeorm migration:generate -f ormconfig.json -d src/migrations",
    "migration:create": "yarn build && typeorm migration:create -f ormconfig.json -d src/migrations",
    "migration:revert": "yarn build && typeorm migration:revert -f ormconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.33",
    "@types/redis": "^4.0.11",
    "connect-redis": "^6.1.3",
    "express-session": "^1.17.3",
    "nodemon": "^2.0.20",
    "redis": "^4.1.0",
    "ts-node": "^10.9.1",
    "tsconfig.json": "^1.0.11",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.1.3",
    "@mikro-orm/core": "^5.1.3",
    "@mikro-orm/migrations": "^5.1.3",
    "@mikro-orm/postgresql": "^5.1.3",
    "@types/bcrypt": "^5.0.0",
    "apollo-server-express": "^3.7.0",
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "ioredis": "^5.0.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "tscpaths": "^0.0.9",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.25"
  }
}
